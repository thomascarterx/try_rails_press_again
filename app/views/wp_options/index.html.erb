<h1>Listing wp_options</h1>

<table>
  <tr>
    <th>Option</th>
    <th>Option name</th>
    <th>Option value</th>
    <th>Autoload</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wp_options.each do |wp_option| %>
  <tr>
    <td><%= wp_option.option_id %></td>
    <td><%= wp_option.option_name %></td>
    <td><%= wp_option.option_value %></td>
    <td><%= wp_option.autoload %></td>
    <td><%= link_to 'Show', wp_option %></td>
    <td><%= link_to 'Edit', edit_wp_option_path(wp_option) %></td>
    <td><%= link_to 'Destroy', wp_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wp option', new_wp_option_path %>
